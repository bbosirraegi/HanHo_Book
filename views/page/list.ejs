
<section>
    <div>
        <p>List</p>
        <% for( var i=0; i < bookList.length; i++ ) { %>
        <div style="border:1px solid black">
            <div>
                <p>
                    이름
                </p>
                <p>
                    <%= bookList[i].book_title %>
                </p>
            </div>
            <div>
                <p>
                    책 설명
                </p>
                <p>
                    <%= bookList[i].book_desc %>
                </p>
            </div>
            <div>
                <p>
                    재고
                </p>
                <p>
                    <%= bookList[i].book_qty %>
                </p>
            </div>
            <div>
                <p>
                    가격
                </p>
                <p>
                    <%= bookList[i].book_price %>
                </p>
            </div>
            <div>
                <p>
                    장르
                </p>
                <p>
                    <%= bookList[i].book_gn %>
                </p>
            </div>

            <div>
                <form method="post" id="listform">
                    <input type="hidden" name="info" value="<%= bookList[i] %>" />
                    <button type="submit" id="submit" name="submit" value="add">장바구니 추가</button>
                    <button type="submit" id="submit" name="submit" value="order">구매</button>
                    <button type="submit" id="submit" name="submit" value="delete">삭제</button>
                </form>
            </div>
        </div>
        <% } %>
    </div>
</section>